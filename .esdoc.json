{
  "source": "./",
  "destination": "./docs",
  "excludes": ["(node_modules|migrations|coverage|test|docs)/"],
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}},
    {"name": "esdoc-jsx-plugin", "option": {"enable": true}},
    {"name": "./docs/esdoc-commonjs"},
    {"name": "./docs/esdoc-readme-badge"}
  ]
}
