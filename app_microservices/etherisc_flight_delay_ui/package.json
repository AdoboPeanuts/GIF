{
  "name": "@etherisc/etherisc_flight_delay_ui",
  "version": "0.1.1",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "webpack",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "setupFiles": [
      "<rootDir>/internals/enzyme-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "evergreen-ui": "^4.5.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.6",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
