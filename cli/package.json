{
  "name": "gifcli",
  "version": "0.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "gifcli": "./bin/run"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "topics": {
      "user": {
        "description": "Manage user"
      },
      "customer": {
        "description": "Manage customers"
      },
      "product": {
        "description": "Manage products"
      },
      "oracle": {
        "description": "Manage oracles"
      }
    },
    "bin": "gifcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@etherisc/amqp": "^1.0.0",
    "@oclif/command": "^1.5.11",
    "@oclif/config": "^1.12.9",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/test": "^1.2.4",
    "cli-ux": "^5.2.0",
    "email-validator": "^2.0.4",
    "fs-jetpack": "^2.2.2",
    "password-validator": "^4.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.2",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0"
  }
}
